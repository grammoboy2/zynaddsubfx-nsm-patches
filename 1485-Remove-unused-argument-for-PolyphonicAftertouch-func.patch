From: Hans Petter Selasky <hps@selasky.org>
Date: Thu, 2 Apr 2020 16:18:27 +0200
Subject: [PATCH 1485/1936] Remove unused argument for PolyphonicAftertouch()
 function.

Signed-off-by: Hans Petter Selasky <hps@selasky.org>
---
 src/Misc/Master.cpp | 2 +-
 src/Misc/Part.cpp   | 5 +----
 src/Misc/Part.h     | 3 +--
 3 files changed, 3 insertions(+), 7 deletions(-)

diff --git a/src/Misc/Master.cpp b/src/Misc/Master.cpp
index d2ae7c10..f5b2133b 100644
--- a/src/Misc/Master.cpp
+++ b/src/Misc/Master.cpp
@@ -977,7 +977,7 @@ void Master::polyphonicAftertouch(char chan, note_t note, char velocity)
         for(int npart = 0; npart < NUM_MIDI_PARTS; ++npart)
             if(chan == part[npart]->Prcvchn)
                 if(part[npart]->Penabled)
-                    part[npart]->PolyphonicAftertouch(note, velocity, keyshift);
+                    part[npart]->PolyphonicAftertouch(note, velocity);
 
     }
     else
diff --git a/src/Misc/Part.cpp b/src/Misc/Part.cpp
index 14302462..61cc2f1b 100644
--- a/src/Misc/Part.cpp
+++ b/src/Misc/Part.cpp
@@ -604,11 +604,8 @@ void Part::NoteOff(note_t note) //release the key
 }
 
 void Part::PolyphonicAftertouch(note_t note,
-				unsigned char velocity,
-				int masterkeyshift)
+				unsigned char velocity)
 {
-    (void) masterkeyshift;
-
     if(!Pnoteon || !inRange(note, Pminkey, Pmaxkey) || Pdrummode)
         return;
 
diff --git a/src/Misc/Part.h b/src/Misc/Part.h
index 722b3bdb..a24e00a0 100644
--- a/src/Misc/Part.h
+++ b/src/Misc/Part.h
@@ -52,8 +52,7 @@ class Part
                     float note_log2_freq) REALTIME;
         void NoteOff(note_t note) REALTIME;
         void PolyphonicAftertouch(note_t note,
-                                  unsigned char velocity,
-                                  int masterkeyshift) REALTIME;
+                                  unsigned char velocity) REALTIME;
         void AllNotesOff() REALTIME; //panic
         void SetController(unsigned int type, int par) REALTIME;
         void ReleaseSustainedKeys() REALTIME; //this is called when the sustain pedal is released
