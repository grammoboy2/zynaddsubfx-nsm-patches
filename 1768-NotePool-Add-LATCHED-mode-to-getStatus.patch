---
 src/Containers/NotePool.cpp | 1 +
 1 file changed, 1 insertion(+)

diff --git a/src/Containers/NotePool.cpp b/src/Containers/NotePool.cpp
index 44ba2ad6..db13c088 100644
--- a/src/Containers/NotePool.cpp
+++ b/src/Containers/NotePool.cpp
@@ -382,6 +382,7 @@ const char *getStatus(int status_bits)
         case 1:  return "PLAY";
         case 2:  return "SUST";
         case 3:  return "RELA";
+        case 4:  return "LTCH";
         default: return "INVD";
     }
 }
